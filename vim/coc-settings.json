{
    "coc.preferences.formatOnSaveFiletypes": ["go", "py"],
    "diagnostic.displayByAle": true,
    "suggest.autoTrigger": "none",
    "languageserver": {
        "golang": {
        "command": "gopls",
        "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
        "filetypes": ["go"],
        "disableWorkspaceFolders": true,
        "initializationOptions": {
            "usePlaceholders": true
        }
    }
  }
}
